{
  "name": "composter-monorepo",
  "private": true,
  "packageManager": "npm@10.9.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "@changesets/cli": "^2.27.0",
    "prettier": "^3.0.0"
  }
  ,
  "overrides": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  }
}